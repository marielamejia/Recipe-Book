<Window x:Class="RecipesStandalone.BuscarNutriologo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RecipesStandalone"
        mc:Ignorable="d"
        Title="BuscarNutriologo" Height="450" Width="800">
    <Grid Background="#FFE1A144">
        <Label Content="Lista de Nutriólogos" HorizontalAlignment="Left" Margin="250,10,0,0" VerticalAlignment="Top" FontSize="30" FontWeight="Bold"/>
        <Button x:Name ="RegresarBus" Content="Regresar" HorizontalAlignment="Left" Margin="593,325,0,0" VerticalAlignment="Top" Width="142" Background="White" FontSize="20" Click="RegresarBus_Click" Height="50" BorderBrush="White" FontWeight="Bold"/>
        <DataGrid x:Name="BuscarNutriDG" HorizontalAlignment="Center" Height="314" Margin="0,80,0,0" VerticalAlignment="Top" Width="262" SelectionChanged="BuscarNutriDG_SelectionChanged"/>

    </Grid>
</Window>
