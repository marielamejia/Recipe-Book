<Window x:Class="RecipesStandalone.BuscarNutriologo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RecipesStandalone"
        mc:Ignorable="d"
        Title="BuscarNutriologo" Height="450" Width="800">
    <Grid Background="#FFFFED00">
        <Label Content="Buscar Nutriólogo" HorizontalAlignment="Left" Margin="250,10,0,0" VerticalAlignment="Top" FontSize="30" FontWeight="Bold"/>
        <Button x:Name ="RegresarBus" Content="Regresar" HorizontalAlignment="Left" Margin="550,350,0,0" VerticalAlignment="Top" Width="75" Background="#FFEBA5A5" FontSize="20" Click="RegresarBus_Click"/>
        <DataGrid x:Name="DGBuscarNutriologo" HorizontalAlignment="Left" Height="100" Margin="350,145,0,0" VerticalAlignment="Top" Width="100" SelectionChanged="DGBuscarNutriologo_SelectionChanged"/>

    </Grid>
</Window>
