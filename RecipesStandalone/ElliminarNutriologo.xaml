<Window x:Class="RecipesStandalone.ElliminarNutriologo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RecipesStandalone"
        mc:Ignorable="d"
        Title="ElliminarNutriologo" Height="450" Width="800">
    <Grid Background="#FFFFED00">
        <Label Content="Eliminar Nutriólogo" HorizontalAlignment="Left" Margin="260,15,0,0" VerticalAlignment="Top" FontSize="30" FontWeight="Bold"/>
        <Label Content="Cédula Prof." HorizontalAlignment="Left" Margin="42,215,0,0" VerticalAlignment="Top" Height="50" Width="143" FontSize="20" FontWeight="Bold"/>
        <TextBox HorizontalAlignment="Left" Height="55" Margin="230,215,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="175" Background="#FF12B7AD" TextChanged="TextBox_TextChanged"/>
        <Button x:Name ="EliminarDel" Content="Eliminar" HorizontalAlignment="Left" Margin="575,275,0,0" VerticalAlignment="Top" Width="145" Background="#FFEBA5A5" Height="50" FontWeight="Bold" FontSize="20"/>
        <Button x:Name ="RegresarDel" Content="Regresar" HorizontalAlignment="Left" Margin="575,350,0,0" VerticalAlignment="Top" Width="145" Background="#FFEBA5A5" Height="51" FontWeight="Bold" FontSize="20"/>

    </Grid>
</Window>
